@model WebApplication1.Models.Employee

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
         
        <table border="1">
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Address</th>
            </tr>
            <tr>
                <td>
                  @Model.m_id
                </td>
                <td>
                    @Model.m_name
                </td>
                <td>
                    @Model.m_address
                </td>
            </tr>
        </table>
    </div>
    <div>
        @helper myList(string[] str)
        { 
           <ul>
               @foreach (var item in str)
	{
		 <li>@item</li>
	}
           </ul> 
           }
        <div>
           <label>Helper Example :------</label> 
            <div>
                @myList(new string [] {"Uday", "Pavan", "piyush", "Chetan"});
            </div>
        </div>
    </div>

    <div>
        @Html.ActionLink("Click Me", "about", new { name = "uday"})    
    </div>
    <div>
        @using (Html.BeginForm("myForm"))
        {
            @Html.TextBoxFor(X => X.m_id);
            <br>
            <br>
            @Html.TextBoxFor(x => x.m_name);
            <br>
            <br>
            @Html.TextBoxFor(x => x.m_address);

            <br>
            <br>
            
            <input value="press me" type="Submit" />


        }
    </div>
    <div>
        @CustomHelper.Image("data:image/jpeg;base64,", "This demo Image");
    </div>
    <div>
        @Html.ImageTag("data:image/jpeg;base64,", "This demo Image");
    </div>

    <h1>
        @ViewBag.MyCustom
    </h1>

    <div>
       <h1>Hcl Name List :---------</h1>
        <ul>

        @foreach (var item in ViewBag.MyList)
        {
           <li>@item</li>
        }
            </ul>
    </div>
    <div>
    <ul>
        @foreach (var item in ViewBag.myEmpList)
        {
            <li>
                Id - @item.m_id , Name - @item.m_name, Add - @item.m_address
            </li>

        }
    </ul>
    </div>
    <h1>
        @ViewData["myList"]
    </h1>
</body>
</html>
